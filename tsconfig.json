{
  "compilerOptions": {
    "target": "es2023",
    "module": "es2022",
    "moduleResolution": "node",
    "lib": ["es2023", "dom"],
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "2024",
    "2025/Template",
    "2025/01",
    "2025/02",
    "2025/03",
    "2025/04",
    "2025/05",
    "2025/06",
    "2025/07"
  ]
}
